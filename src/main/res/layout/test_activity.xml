<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/province_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="解析省份数据并存进数据库"/>
    <Button
        android:id="@+id/load_province_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="查询省份数据并显示"/>
    <Button
        android:id="@+id/city_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="解析城市数据并存进数据库"/>
    <Button
        android:id="@+id/load_city_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="查询城市数据并显示"/>
    <Button
        android:id="@+id/county_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="解析县/区数据并存进数据库"/>
    <Button
        android:id="@+id/load_county_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="查询县/区数据并显示"/>
    <Button
        android:id="@+id/http_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="测试Http请求"/>
</LinearLayout>